        $(document).ready(function () {
            
            
            // Initialize the advertId object.
            var advertId = 3735318;
            var isInShortlist = false;

            if(isInShortlist){
                $("#save-listing").hide();
                $("#remove-listing").show();
            }

            function addToShortlistCounter(){
                var currentCount = parseInt($("#shortlist-count").text());
                $("#shortlist-count").text(currentCount+1);
            }

            function subtractFromShortlistCounter(){
                var currentCount = parseInt($("#shortlist-count").text());
                $("#shortlist-count").text(currentCount-1);
            }

            /**
            * Handle a user clicking the save-listing to shortlist button. Assumes that #save-listing
            * is visible.
            **/
            $("#save-listing").on('click', function (event) {
                var loginUrl = $(this).attr('href');

                event.preventDefault();
                $("#save-listing").hide();
                $("#shortlist-loading").show();
                // Perform AJAX POST of advertId to save-listing page.
                $.ajax({
                    url: 'api/shortlist/' + 3735318,
                    type: 'PUT',
                    cache: false,
                    success: function (data, textStatus, xhr) {
                        $("#shortlist-loading").hide();
                        if(typeof(data.message) !== 'undefined'){
                            console.log(data.message);
                            // Added to Shortlist OK. Show the reverse option.
                            $("#remove-listing").show();
                            addToShortlistCounter();
                        } else {
                            // Server has responded with OK but data is not what we expect.
                            $("#shortlist-fail").show();
                            // Allow the user to try again.
                            $("#save-listing").show();
                        }
                    },
                    error: function (response) {
                        // Server has responded with error. Re-direct to login page if 401.
                        if (response.status === 401 || response.status === 403){
                            // User is not authorized to perform this operation (not logged in).
                            window.location.href = loginUrl;
                        }
                    }
                });
            });
            /**
            * Handle a user clicking the remove-listing from shortlist button. Assumes that #remove-listing
            * is visible.
            **/
            $("#remove-listing").on('click', function (event) {
                event.preventDefault();

                var loginUrl = $(this).attr('href');

                $("#remove-listing").hide();
                $("#shortlist-loading").show();
                // Perform AJAX POST of advertId to save-listing page.
                $.ajax({
                    url: 'api/shortlist/' + 3735318,
                    type: 'DELETE',
                    cache: false,
                    success: function (result) {
                        $("#shortlist-loading").hide();
                        if(typeof(result.message) !== 'undefined'){
                            // Removed from Shortlist OK. Show the reverse option.
                            $("#save-listing").show();
                            // Subtract one from..
                            subtractFromShortlistCounter();
                        } else {
                            // Server error. Not OK.
                            $("#shortlist-fail").show();
                            // Allow the user to try again.
                            $("#remove-listing").show();
                        }
                    },
                    error: function(response){
                        // Server has responded with error. Re-direct to login page if 403.
                        if(response.status === 401){
                            // User is not authorized to perform this operation (not logged in).
                            window.location.href = loginUrl;
                        }
                    }
                });
            });

        });
    </script>



